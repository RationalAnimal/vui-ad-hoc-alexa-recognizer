    "chai": "^3.5.0",
    "express": "^4.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "vui",
    "nlu",
    "natural language understanding",
    "nlp",
    "natural language processing",
    "alexa",
    "cortana",
